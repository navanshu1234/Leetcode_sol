List<List<Integer>> ar = new ArrayList<>();
        for(int i=0;i<nums.length;i++)
        {   ArrayList<Integer> arr = new ArrayList<>();
            for(int j=i;j<nums.length;j++)
            {   
                arr.add(nums[j]);
                ar.add(new ArrayList(arr));
            }
            
        }
        System.out.println(ar);
